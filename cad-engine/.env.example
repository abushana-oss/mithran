# ============================================================================
# CAD Engine - Environment Variables
# ============================================================================

# Application
PORT=5000
NODE_ENV=development
LOG_LEVEL=info

# Security
CORS_ORIGINS=http://localhost:3000,http://localhost:4000
MAX_FILE_SIZE_MB=50
RATE_LIMIT_PER_MINUTE=10

# Conversion Settings
LINEAR_DEFLECTION=0.1
ANGULAR_DEFLECTION=0.5

# Storage
TMPDIR=/tmp/cad-files
